{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Parser",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/parsers/run_parser.py",
            "args": [
                "/Users/bogdan/Code/olmocr/olmOCR-bench/bench_data_med/pdfs",
                "llamaparse",
                "--config",
                "agent_haiku45",
                "-r",
                "--extensions",
                ".pdf",
                "--max-concurrency",
                "50"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Run Benchmark",
            "type": "debugpy",
            "request": "launch",
            "module": "olmocr.bench.benchmark",
            "args": [
                "--md_folder",
                "/Users/bogdan/Code/olmocr/runs/20251031_150035_llamaparse_agent_haiku45",
                "--test_report",
                "--skip_math",
                "--force",
                // "--sample",
                // "50",
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}"
        }
    ]
}
